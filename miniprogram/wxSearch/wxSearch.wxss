.wxSearch{ 
  position:absolute; 
  margin-top:370rpx; 
  left: 0px; 
  width: 100%; 
} 
.wxSearchInner{ 
  position:absolute; 
  left: 20rpx; 
  text-align: center; 
  text-shadow: gray; 
  text-transform: uppercase; 
  width: 700rpx; 
  height: 180rpx; 
  margin: auto; 
} 
.wxSearchInner2{ 
  position:absolute; 
  left: 20rpx; 
  text-align: center; 
  text-shadow: gray; 
  text-transform: uppercase; 
  width: 700rpx; 
  height: 400rpx; 
  margin: auto; 
} 
.wxSearch-section{ 
  position:relative; 
  top:100rpx; 
  background-color: rgb(255, 255, 255); 
  height: 102rpx; 
  width: 700rpx;  
} 
.wxSearch-pancel{ 
  display: flex; 
  flex-direction: row; 
} 
.wxSearch-input{ 
  flex: 8;  
  background-color: rgb(231, 228, 228); 
  width: 400rpx; 
  height: 100rpx; 
  padding-left: 50rpx; 
  border:3rpx solid rgb(206, 206, 206); 
} 
.wxSearch-button{ 
  flex: 1; 
  border: none !important; 
  color: cornflowerblue !important; 
  margin-top: 25rpx; 
} 
 
.wxSearchTitle{ 
  display: block; 
  padding: 10rpx 5rpx 5rpx 10rpx; 
  font-size: 35rpx; 
  text-align: center; 
  font-weight: bolder; 
  color : rgb(90, 90, 90) 
} 
 
 
.wxSearchMindKeyItem{ 
  padding: 10rpx 5rpx; 
  border-bottom: 1rpx solid #eee; 
  display: flex; 
  font-size: 14rpx; 
  color: cornflowerblue; 
} 
 
.wxSearchKeyList{ 
  display: flex; 
  flex-direction: row; 
  flex-wrap: wrap; 
} 
 
.wxSearchKeyItem{ 
  flex: 0 0 20%; 
  text-align: center; 
  border: 1rpx solid rgb(226, 226, 226); 
  margin: 5rpx; 
  padding: 5rpx 10rpx; 
  border-radius: 5rpx; 
  font-size: 25rpx; 
  font-stretch: wider; 
  color: #5f5f5f;
  font-size-adjust: 0.6; 
  font-family: Arial, Helvetica, sans-serif; 
} 
.wxSearchHistoryItem{ 
  padding: 10rpx 5rpx; 
  font-size: 25rpx; 
  border-bottom: 1rpx solid #eee; 
  display: flex; 
} 
.wxSearchHistoryItemText{ 
  flex: 8; 
  font-size: 20rpx; 
} 
.wxSearchHistoryItemDel{ 
  flex: 1; 
  font-size: 18rpx; 
  text-align: center; 
  padding: 3rpx 5rpx; 
  border: 1rpx solid #eee; 
  border-radius: 5rpx; 
} 
.wxSearchHistoryEmpty{ 
  text-align: center; 
  height: 50rpx; 
  line-height: 50rpx; 
  font-size: 25rpx; 
  color:#5f5f5f;
} 
.wxSearchDeleteAllBtn{ 
  position:relative;
  top: 5rpx;
  height: 50rpx; 
  left:230rpx;
  line-height: 50rpx; 
  width:220rpx;
  text-align: center;
  border: 1rpx solid #eee;  
  font-size: 25rpx; 
}
