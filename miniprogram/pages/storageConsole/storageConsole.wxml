<!--pages/storageConsole/storageConsole.wxml-->
<view class="page">
  <view class="container" mode="aspectFit" >
   <!-- 显示查询图拍-->
    <image class="img" src="{{imagePath}}"/>
    <!-- 显示查询到的分类结果-->
    <view class="msgs" wx:for="{{msgs}}">
      {{msgs[index]}}
    </view>
    <!-- 显示查询到的按钮们-->
    <view class='attrs' wx:for='{{categories}}' wx:for-index='key'>
      <block wx:if="{{item > 0}}">
        <button size='default' disabled='{{disabled}}' type='primary'>{{key}}</button>
      </block>
    </view>
  </view>
</view>
