.personal_info {
  position: relative;
  width: 750rpx;
  height: 300rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.personal_info-avatar {
  overflow:hidden;
  display: block;
  width: 150rpx;
  height: 150rpx;
  margin: 10rpx;
  margin-top: 50rpx;
  border-radius: 50%;
  border: 2px solid rgb(255, 255, 255);
  box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);
}
.personal_info-name {
  font-family: STKaiti;
  font-size: 43rpx;
  color: rgb(32, 18, 109);
}

.personal-record {
  position: relative;
  width: 750rpx;
  height: 230rpx;
  display: flex;
  flex-direction: row;
  justify-content:center;
  align-items:center;
}
.placehd{
    margin: 0px;
    padding: 0 10px;
    text-align: center;
}
.record_title {
    color: black;
    font-family: STKaiti;
    font-size: 40rpx;
}
.record_content {
    color: rgba(0, 0, 0, 0.671);
    font-family: STKaiti;
    font-size: 36rpx;
}

.icon_view {
  width: 80rpx;
  height: 80rpx;
  border-radius: 100%;
}

.aweui-grids{
  border-top:1rpx;
  border-left:1rpx;
  overflow:hidden
}
.aweui-grid {
  position:relative;
  float:left;
  padding:20px 10px;
  width:33.33333333%;
  box-sizing:border-box;
  border-right:1rpx;
  border-bottom:1rpx;
}
.aweui-grid_active{
  background-color:#ececec;
}
.aweui-grid__icon{
  display:block;
  width:39px;
  height:39px;
  margin:0 auto;
}
.aweui-grid__label{
  margin-top:5px;
  display:block;
  text-align:center;
  color:#000;
  white-space:nowrap;
  text-overflow:ellipsis;
  overflow:hidden;
  font-size: 36rpx;
  font-family: STKaiti;
}


